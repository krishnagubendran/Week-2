/*
Q19: Product of Digits [Easy]
- Input: 123
Output: 6
- Input: 405
Output: 0
*/
package week2;
import java.util.Scanner;
public class ProductOfDigits {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int prod =1, digit;
		while(n>0) {
			digit = n % 10;
			prod *= digit;
			n /= 10;					
		}
		System.out.println(prod);
		sc.close();
	}
}
