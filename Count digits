/*
Q3: Count Digits [Easy]
- Input: 98765
Output: 5
- Input: 123456789
Output: 9
*/
package week2;
import java.util.Scanner;
public class CountDigits {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int count=0;
		while(n>0) {
			n /= 10;
			count++;
		}
		System.out.println(count);
		sc.close();
	}
}
