/*
Q20: Sum of Even Digits [Easy]
- Input: 123456
Output: 12
- Input: 789
Output: 8 
*/
package week2;
import java.util.Scanner;
public class SumOfEvenDigits {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int sum =0, digit;
		while(n >0) {
			digit = n % 10;
			if(digit %2 ==0) sum+= digit;
			n /= 10;
		}
		System.out.println(sum);
		sc.close();
	}
}
